<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    android:weightSum="1">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="189dp"
        android:layout_height="43dp"
        app:srcCompat="@drawable/schedule_text"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="7dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.502" />

    <Button
        android:id="@+id/btnCall"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@drawable/phone_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toLeftOf="@+id/btnSchedule"
        android:layout_marginRight="4dp" />

    <Button
        android:id="@+id/btnLogin"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@drawable/account_logo"
        android:layout_marginLeft="4dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toLeftOf="@+id/btnMap"
        android:layout_marginRight="4dp" />

    <Button
        android:id="@+id/btnMap"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@drawable/maps_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintRight_toLeftOf="@+id/btnCall"
        android:layout_marginRight="4dp" />

    <Button
        android:id="@+id/btnSchedule"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:background="@drawable/schedule_logo"
        android:layout_marginRight="4dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/add_event_routine"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/button4"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.25"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/button4"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/search_event_routine"
        android:layout_marginLeft="308dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />


</android.support.constraint.ConstraintLayout>